{
  "name": "fempalette-sass-tutorial",
  "version": "1.0.0",
  "description": "Generador de paletas de colores interactivo con tutorial completo de SASS",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=8080 --open=index.html",
    "build:sass": "sass styles/main.scss:styles/main.css --style=compressed",
    "watch:sass": "sass --watch styles/main.scss:styles/main.css",
    "build:sass:example": "sass example-sass-architecture.scss:compiled-example.css",
    "watch:sass:example": "sass --watch example-sass-architecture.scss:compiled-example.css",
    "serve": "python -m http.server 8000",
    "dev": "concurrently \"npm run watch:sass\" \"npm run start\"",
    "build": "npm run build:sass"
  },
  "keywords": [
    "sass",
    "scss",
    "css",
    "color-palette",
    "tutorial",
    "education",
    "frontend",
    "femcoders",
    "web-development"
  ],
  "author": "femCoders Club",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "sass": "^1.89.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/femcodersclub/fempalette-sass"
  },
  "bugs": {
    "url": "https://github.com/femcodersclub/fempalette-sass/issues"
  },
  "homepage": "https://femcodersclub.com",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
